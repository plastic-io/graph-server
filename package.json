{
  "name": "communications-platform-orchestration-poc",
  "version": "1.0.0",
  "description": "Basically S3 CRUD functions.",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build-docs": "./node_modules/.bin/typedoc"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.55",
    "aws-sdk": "^2.632.0",
    "handlebars": "^4.5.0",
    "jest": "^22.4.4",
    "serverless": "^1.69.0",
    "serverless-cloudfront-distribution-certificate": "0.0.7",
    "serverless-webpack": "^4.4.0",
    "ts-loader": "^3.5.0",
    "typedoc": "^0.16.11",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0"
  },
  "author": "Tony.Germaneri@patientpop.com",
  "dependencies": {
    "deep-diff": "^1.0.2",
    "jshashes": "^1.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "testURL": "http://localhost",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "dist/**/*.js"
    ]
  }
}
